   <div class="preloader">
      <span></span>
      <span></span>
   </div>
</div>
<!-- preloader end  -->





<!-- main-area-start -->
<main>

   <!-- product-area-start -->


   {{!-- banner --}}

   <section style="margin-top:50px ; ">
      <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" style="width:50% ;">
         <div class="carousel-inner">
            <div class="carousel-item active">
               <img class="d-block w-100" src="https://media.idownloadblog.com/wp-content/uploads/2019/09/Apple-Innovation-Event-banner.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
               <img class="d-block w-100" src="	https://media.idownloadblog.com/wp-content/uploads/2020/11/Apple-november-event-banner.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
               <img class="d-block w-100" src="https://cdn.appleosophy.com/2020/11/IMG_7382.jpg" alt="Third slide">
            </div>
         </div>
         <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
         </a>
         <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
         </a>
      </div>
   </section>

   {{#if data}}
   <section class="product-area pt-95 pb-70">
      <div class="container">
         <div class="row">
            <div class="col-md-6 col-12">
               <div class="tpsection mb-40">
                  <h4 class="tpsection__title">Favorite <span> Products<i class="bi bi-suit-heart"></i><img
                           src="/img/icon/title-shape-01.jpg" alt=""></span></h4>
               </div>
            </div>
         </div>



         <div class="tab-content" id="nav-tabContent">
            <div class="tab-pane fade show active" id="nav-all" role="tabpanel" aria-labelledby="nav-all-tab">
               <div class="row row-cols-xxl-5 row-cols-xl-4 row-cols-lg-3 row-cols-md-2 row-cols-sm-2 row-cols-1">

                  {{#each data}}
                  <div class="col">
                     <div class="tpproduct pb-15 mb-30">
                        <div class="tpproduct__thumb p-relative">
                           <img src="/product-images/{{this.products.images.[0]}}" alt="product-thumb">
                           <img class="product-thumb-secondary" src="/product-images/{{this.products.images.[1]}}"
                              alt="">
                           <div class="tpproduct__thumb-action">
                              <a class="quckview" href="#"><i class="fal fa-eye"></i></a>
                              <a class="wishlist" href="/view-wishlist"><i class="fal fa-heart"></i></a>
                           </div>
                        </div>
                        <div class="tpproduct__content">
                           <h3 class="tpproduct__title"><a href="shop-details.html"
                                 style="font-weight: bold ; color:black ;">{{this.products.ProductName}}</a>
                           </h3>
                           <div class="tpproduct__priceinfo p-relative">
                              <div class="tpproduct__priceinfo-list">
                                 <span>{{this.products.Price}}</span>
                              </div>
                              <div class="tpproduct__cart">
                                 <a href="/view-cart-items"><i class="fal fa-shopping-cart"></i>Add To Cart</a>
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
                  {{/each}}
               </div>
            </div>
         </div>

   </section>
   <!-- product-area-end -->
   {{/if}}

   <!-- category-area-start -->
   <section class="category-area pt-70">
      <div class="container">
         <div class="row">
            <div class="col-md-12">
               <div class="tpsection mb-40">
                  <h4 class="tpsection__title">Top <span> Categories <img src="/img/icon/title-shape-01.jpg"
                           alt=""></span></h4>
               </div>
            </div>
         </div>
         <div class="custom-row category-border pb-45 justify-content-xl-between">
            <div class="tpcategory mb-40">
               <div class="tpcategory__icon p-relative">
                  <h2 style="text-align:center ;"><img src="https://cdn-icons-png.flaticon.com/512/35/35367.png" alt=""
                        style="width: 6rem ;"></i></h2>
                  <span>20</span>
               </div>
               <div class="tpcategory__content">
                  <h5 class="tpcategory__title"><a href="shop.html">Driftwood <br> Table Decor</a></h5>
               </div>
            </div>
         </div>
      </div>
   </section>
   <!-- category-area-end -->



   <!-- shop-instagrame-start -->
   <section class="shop-area pb-100">
      <div class="container">
         <div class="row">
            <div class="col-md-12">
               <div class="tpsectionarea text-center mb-35">
                  <h5 class="tpsectionarea__subtitle">Follow On</h5>
                  <h4 class="tpsectionarea__title"><i class="fab fa-instagram"></i> ninico-shop</h4>
               </div>
            </div>
         </div>
      </div>
   </section>
   <!-- shop-instagrame-end -->

</main>
<!-- main-area-end -->

<!-- footer-area-start -->