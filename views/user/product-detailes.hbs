                        </div>
                        <div class="tpproduct-details__cart ml-20">
                           <button type="submit"><i class="fal fa-shopping-cart"></i> Add To
                              Cart</button>
                        </div>
                  </form>
                  {{/ifEquals}}


                  <div class=" ml-20">
                     <button class="btn btn-social-icon btn-outline-dribbble" type="button"
                        onclick="addToWishlist('{{product._id}}')"><i class="fal fa-heart"></i></button>
                  </div>

