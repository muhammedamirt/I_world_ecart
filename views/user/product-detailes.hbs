                  </div>
                  <div class="tpproduct-details__pera">
                     <p>{{product.Description}}</p>
                  </div>
                  {{#ifEquals product.stock '0' }}
                  <h5 style="color:rgb(128, 0, 0) ;">Out Of Stock</h5>
                  {{else}}
